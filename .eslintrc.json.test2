{
  "extends": [
    "react-app"
  ],
  "plugins": [
    "prettier",
    "jsx-a11y",
    "pretter",
    "react"
  ],
  "rules": {
    "semi": 0,
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "prettier/prettier": [
      "error", {
        "semi": false
      }
    ]
  }
}