{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "airbnb"
  ],
  "overrides": [
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "react"
  ],
  "rules": {
    //      "indent": [4, "tab", { "SwitchCase": 1, "VariableDeclarator": 1 }]
    "indent": [
      "error",
      4
    ],
    "react/jsx-indent": [
      "error",
      4
    ],
    "react/jsx-indent-props": [
      "error",
      4
    ],
    "quotes": [
      "error",
      "double"
    ],
    "semi": [
      "error",
      "always"
    ],
    "react/prop-types": 0,
    "linebreak-style": [
      "error",
      "windows"
    ],
    "camelcase": "off",
    "react-no-array-index-key": 0,
    "no-unused-vars": "off",
    "no-restricted-syntax": "off"
  }
}
